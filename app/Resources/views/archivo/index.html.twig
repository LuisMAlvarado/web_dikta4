{% extends 'base.html.twig' %}

{% block body %}
    <h1>Archivos list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Factor1</th>
                <th>Createat</th>
                <th>Pdf</th>
                <th>Descripcion</th>
                <th>Isactive</th>
                <th>Isdelete</th>
                <th>Updateat</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for archivo in archivos %}
            <tr>
                <td><a href="{{ path('archivo_show', { 'id': archivo.id }) }}">{{ archivo.id }}</a></td>
                <td>{{ archivo.factor1 }}</td>
                <td>{% if archivo.createAt %}{{ archivo.createAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ archivo.pdf }}</td>
                <td>{{ archivo.descripcion }}</td>
                <td>{% if archivo.isActive %}Yes{% else %}No{% endif %}</td>
                <td>{% if archivo.isDelete %}Yes{% else %}No{% endif %}</td>
                <td>{% if archivo.updateAt %}{{ archivo.updateAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('archivo_show', { 'id': archivo.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('archivo_edit', { 'id': archivo.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('archivo_new') }}">Create a new archivo</a>
        </li>
    </ul>
{% endblock %}
